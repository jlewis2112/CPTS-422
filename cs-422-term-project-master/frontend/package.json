{
  "name": "apartment-frontend",
  "version": "0.1.0",
  "proxy": "http://localhost:5000",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "mdbreact": "^4.27.0",
    "msw": "^0.21.3",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^4.0.0",
    "react-slideshow-image": "3.1.0",
    "react-test-renderer": "^17.0.1"
  },
  "scripts": {
    "start": "npm run watch:css && react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test --testURL=http://localhost:5000",
    "eject": "react-scripts eject",
    "build:css": "postcss src/assets/tailwind.css -o src/assets/AUTOGENERATED_DONT_EDIT.css",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/AUTOGENERATED_DONT_EDIT.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^7.26.3",
    "@testing-library/user-event": "^7.2.1",
    "autoprefixer": "9.8.6",
    "postcss-cli": "^8.0.0",
    "tailwindcss": "^1.8.10"
  }
}
